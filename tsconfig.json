{
  "compilerOptions": {
    "lib": ["es6", "dom", "dom.iterable"],
    "module": "commonjs",
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "strict": true,
    "target": "es5",
    "esModuleInterop": true,
    "importHelpers": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "compileOnSave": true,
  "include": [
    "src/**/*",
    "misc/**/*"
  ],
  "exclude": [
    "node_modules",
    "misc/predict.ts"
  ]
}
